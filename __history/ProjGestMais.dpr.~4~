program ProjGestMais;



uses
  Vcl.Forms,
  uFrmMenuPrincipal in 'uFrmMenuPrincipal.pas' {FrmMenuPrincipal},
  uFrmLogin in 'uFrmLogin.pas',
  uFrmPesquisa in 'uFrmPesquisa.pas' {FrmPesquisa},
  uDM in 'uDM.pas' {FrmDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFrmMenuPrincipal, FrmMenuPrincipal);
  Application.CreateForm(TFrmPesquisa, FrmPesquisa);
  Application.CreateForm(TFrmDataModule, FrmDataModule);
  Application.Run;
end.
